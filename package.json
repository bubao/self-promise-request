{
	"name": "self-promise-request",
	"version": "0.0.10",
	"description": "自己封装的一个request，使用promise，并能监听下载进度。",
	"main": "index.js",
	"dependencies": {
		"cli-color": "^2.0.0",
		"request": "^2.88.2",
		"single-line-log": "^1.1.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"commitizen": "^4.0.3",
		"cz-customizable": "^6.2.0",
		"eslint": "^7.19.0",
		"eslint-config-standard": "^16.0.2",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-json": "^2.0.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^5.0.0",
		"husky": "^4.3.8"
	},
	"author": "bubao",
	"license": "MIT",
	"husky": {
		"hooks": {
			"pre-commit": "eslint --fix .",
			"pre-push": "eslint --fix .",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		},
		"cz-customizable": {
			"config": "config/cz-config.js"
		}
	},
	"engines": {
		"node": ">=8.3.0"
	},
	"directories": {
		"test": "test"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bubao/self-promise-request.git"
	},
	"keywords": [
		"request"
	],
	"bugs": {
		"url": "https://github.com/bubao/self-promise-request/issues"
	},
	"homepage": "https://github.com/bubao/self-promise-request#readme"
}
